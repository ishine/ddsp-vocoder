{
    "preprocess": {
        "length": 48000,
        "pitch_estimation": "fcpe",
        "sample_rate": 48000,
        "n_fft": 1920,
        "frame_size": 480,
        "n_mels": 80
    },
    "trainer": {
        "devices": "auto",
        "max_epochs": 10000,
        "precision": "bf16-mixed",
        "log_every_n_steps": 50
    },
    "data_module": {
        "cache_dir": "dataset_cache",
        "batch_size": 4,
        "num_workers": 1
    },
    "save": {
        "interval": 400,
        "models_dir": "models"
    },
    "model": {
        "generator": {
            "n_mels": 80,
            "sample_rate": 48000,
            "frame_size": 480,
            "internal_channels": 256,
            "num_layers": 4,
            "n_fft": 1920
        },
        "discriminator": {
            "mpd": {
                "periods": [1, 2, 3, 5, 7, 11],
                "channels": 32,
                "channels_mul": 2,
                "max_channels": 256,
                "num_layers": 4
            },
            "mrd": {
                "resolutions": [128, 256, 512],
                "channels": 32,
                "num_layers": 6
            }
        }
    }
}