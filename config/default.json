{
    "preprocess": {
        "length": 48000,
        "pitch_estimation": "fcpe",
        "sample_rate": 48000,
        "n_fft": 1920,
        "frame_size": 480,
        "n_mels": 80
    },
    "trainer": {
        "devices": "auto",
        "max_epochs": 10000,
        "precision": "bf16-mixed",
        "log_every_n_steps": 20
    },
    "data_module": {
        "cache_dir": "dataset_cache",
        "batch_size": 16,
        "num_workers": 1
    },
    "save": {
        "interval": 400,
        "models_dir": "models"
    },
    "model": {
        "generator": {
            "n_mels": 80,
            "sample_rate": 48000,
            "frame_size": 480,
            "internal_channels": 256,
            "num_layers": 6,
            "n_fft": 1920
        },
        "discriminator": {
            "scales": [1, 2, 4],
            "channels": 32,
            "num_layers": 4
        }
    }
}